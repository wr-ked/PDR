<?xml version="1.0" encoding="UTF-8"?>

<launch> 

<include file="$(find turtle_tf_3d)/launch/spawn_sdf.launch">
    <arg name="sdf_robot_file" value="$(find turtle_tf_3d)/models/turtle1/model.sdf"/>
    <arg name="robot_name" value="turtle1" />
    <arg name="x" default="0.0" />
    <arg name="y" default="0.0" />
    <arg name="z" default="1.0" />
</include>

<include file="$(find turtle_tf_3d)/launch/spawn_sdf.launch">
    <arg name="sdf_robot_file" value="$(find turtle_tf_3d)/models/turtle2/model.sdf"/>
    <arg name="robot_name" value="turtle2" />
    <arg name="x" default="3.0" />
    <arg name="y" default="0.0" />
    <arg name="z" default="1.0" />
</include>

</launch>